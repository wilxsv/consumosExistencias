{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ctlinsumo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ctlInsumo.id }}</td>
            </tr>
            <tr>
                <th>Codigonu</th>
                <td>{{ ctlInsumo.codigoNu }}</td>
            </tr>
            <tr>
                <th>Codigosinab</th>
                <td>{{ ctlInsumo.codigoSinab }}</td>
            </tr>
            <tr>
                <th>Codigoecri</th>
                <td>{{ ctlInsumo.codigoEcri }}</td>
            </tr>
            <tr>
                <th>Codigoatc</th>
                <td>{{ ctlInsumo.codigoAtc }}</td>
            </tr>
            <tr>
                <th>Listadooficial</th>
                <td>{% if ctlInsumo.listadoOficial %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Nombrelargo</th>
                <td>{{ ctlInsumo.nombreLargo }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('insumo_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
